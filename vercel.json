{
  "buildCommand": "pnpm install && pnpm build",
  "outputDirectory": "client/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless"
    }
  ],
  "functions": {
    "api/serverless.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
