{

  "version": 2,

  "builds": [

    {

      "src": "client/package.json",

      "use": "@vercel/static-build",

      "config": {

        "distDir": "dist"

      }

    },

    {

      "src": "server/_core/index.ts",

      "use": "@vercel/node"

    }

  ],

  "routes": [

    {

      "src": "/api/(.*)",

      "dest": "server/_core/index.ts"

    },

    {

      "src": "/(.*)",

      "dest": "client/dist/$1"

    }

  ]

}